# traditional IPv4 address for localhost
127.0.0.1       localhost ipv4-localhost ipv4-loopback loopback localhost.localdomain

# special IPv6 addresses
::1             localhost ipv6-localhost ipv6-loopback ip6-localhost ip6-loopback

fe00::0         ipv6-localnet ip6-localnet

ff00::0         ipv6-mcastprefix ip6-mcastprefix
ff02::1         ipv6-allnodes ip6-allnodes
ff02::2         ipv6-allrouters ip6-allrouters
ff02::3         ipv6-allhosts





